<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="VES\FeaturedProduct\Block\Product\ListProduct" name="featuredproduct.block.list" template="VES_FeaturedProduct::list.phtml" cacheable="false" />
        </referenceContainer>
        <referenceContainer name="after.body.start">
            <block class="Magento\Framework\View\Element\Js\Components" name="head.components" as="components" template="VES_FeaturedProduct::js/components.phtml"/>
        </referenceContainer>
    </body>
</page>
