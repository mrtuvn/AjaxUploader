<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">

                <resource id="Tuna_BannerSlider::bannerslider_bannerslider" title="Banner Slider" sortOrder="10">
                    <resource id="Tuna_BannerSlider::bannerslider_sliders" title="Manager Sliders" sortOrder="10" />
                    <resource id="Tuna_BannerSlider::bannerslider_banners" title="Manager Banners" sortOrder="20" />
                    <resource id="Tuna_BannerSlider::bannerslider_allcoreslider" title="Preview Sliders" sortOrder="30" />
                    <resource id="Tuna_BannerSlider::settings" title="Settings" sortOrder="40"/>
                </resource>

                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">

                            <resource id="Tuna_BannerSlider::config_bannerslider" title="Banner Slider" />

                        </resource>
                    </resource>

                </resource>
            </resource>
        </resources>
    </acl>
</config>
